<wpml-config>
    <elementor-widgets>

        <widget name="premium-behance-feed">
            <fields>
                <field type="Behance Feed: Username" editor_type="LINE">username</field>
            </fields>
        </widget>

        <widget name="premium-chart">
            <fields>
                <field type="Charts: X-axis Label" editor_type="LINE">x_axis_label</field>
                <field type="Charts: Data Labels" editor_type="LINE">x_axis_labels</field>
                <field type="Charts: y-axis Label" editor_type="LINE">y_axis_label</field>
                <field type="Charts: Title" editor_type="LINE">title</field>
            </fields>
            <fields-in-item items_of="y_axis_data">
                <field type="Charts: Y-axis Data Title" editor_type="LINE">y_axis_column_title</field>
                <field type="Charts: Y-axis Data URLs" editor_type="LINE">y_axis_urls</field>
                <field type="Charts: Y-axis Data Data" editor_type="LINE">y_axis_column_data</field>
            </fields-in-item>
        </widget>

        <widget name="premium-addon-content-toggle">
            <fields>
                <field type="Content Switcher: First Label" editor_type="LINE">premium_content_toggle_heading_one</field>
                <field type="Content Switcher: Second Label" editor_type="LINE">premium_content_toggle_heading_two</field>
                <field type="Content Switcher: First Template ID" editor_type="LINE">premium_content_toggle_first_content_templates</field>
                <field type="Content Switcher: Second Template ID" editor_type="LINE">premium_content_toggle_second_content_templates</field>
                <field type="Content Switcher: First Content" editor_type="AREA">premium_content_toggle_first_content_text</field>
                <field type="Content Switcher: Second Content" editor_type="AREA">premium_content_toggle_second_content_text</field>
            </fields>
        </widget>

        <widget name="premium-divider">
            <fields>
                <field type="Divider: Separator Text" editor_type="LINE">content_text</field>
                <field type="Divider: Link Title" editor_type="LINE">content_link_title</field>
                <field type="Divider: Separator Link" editor_type="LINK">content_url>url</field>
            </fields>
        </widget>

        <widget name="premium-facebook-feed">
            <fields>
                <field type="Facebook Feed: Access Token" editor_type="LINE">access_token</field>
                <field type="Facebook Feed: User ID/Page Slug" editor_type="LINE">account_id</field>
                <field type="Facebook Feed: Show more Text" editor_type="LINE" >read_text</field>
            </fields>
        </widget>

        <widget name="premium-facebook-reviews">
            <fields>
                <field type="Facebook Reviews: Page Name" editor_type="LINE">page_name</field>
            </fields>
        </widget>

        <widget name="premium-fb-chat">
            <fields>
                <field type="Messanger Chat: Login Message" editor_type="LINE">premium_fbchat_login_msg</field>
                <field type="Messanger Chat: Logout Message" editor_type="LINE">premium_fbchat_logout_msg</field>
            </fields>
        </widget>

        <widget name="premium-addon-flip-box">
            <fields>
                <field type="Hover Box: Front Title" editor_type="LINE">premium_flip_paragraph_header</field>
                <field type="Hover Box: Back Title" editor_type="LINE">premium_flip_back_paragraph_header</field>
                <field type="Hover Box: Front Description" editor_type="AREA">premium_flip_paragraph_text</field>
                <field type="Hover Box: Back Description" editor_type="AREA">premium_flip_back_paragraph_text</field>
                <field type="Hover Box: Link Text" editor_type="LINE">premium_flip_back_link_text</field>
                <field type="Hover Box: URL" editor_type="LINK">premium_flip_back_link>url</field>
            </fields>
        </widget>

        <widget name="premium-google-reviews">
            <fields>
                <field type="Google Reviews: API Key" editor_type="LINE">api_key</field>
                <field type="Google Reviews: Place ID" editor_type="LINE">place_id</field>
                <field type="Google Reviews: Language Prefix" editor_type="LINE">language_prefix</field>
            </fields>
        </widget>

        <widget name="premium-addon-icon-box">
            <fields>
                <field type="Icon Box: Title" editor_type="LINE">premium_icon_box_title</field>
                <field type="Icon Box: Title" editor_type="LINE">premium_icon_box_label</field>
                <field type="Icon Box: Description" editor_type="AREA">premium_icon_box_content</field>
                <field type="Icon Box: Read More Text" editor_type="LINE">premium_icon_box_more_text</field>
                <field type="Icon Box: URL" editor_type="LINK">premium_icon_box_link>url</field>
            </fields>
        </widget>

        <widget name="premium-ihover">
            <fields>
                <field type="iHover: Link Title" editor_type="LINE">premium_ihover_thumbnail_link_text</field>
                <field type="iHover: Link Title" editor_type="LINE">premium_ihover_thumbnail_back_title</field>
                <field type="iHover: Link Descrtiption" editor_type="AREA">premium_ihover_thumbnail_back_description</field>
                <field type="iHover: URL" editor_type="LINK">premium_ihover_thumbnail_url>url</field>
            </fields>
        </widget>

        <widget name="premium-image-accordion">
            <fields-in-item items_of="image_content">
                <field type="Image Accordion: Image Title" editor_type="LINE">image_title</field>
                <field type="Image Accordion: Image Description" editor_type="AREA">image_desc</field>
                <field type="Image Accordion: Link Title" editor_type="LINE">link_title</field>
                <field type="Image Accordion: Content Template ID" editor_type="LINE">temp_content</field>
                <field type="Image Accordion: Image Link" editor_type="LINK">link>url</field>
            </fields-in-item>
        </widget>

        <widget name="premium-addon-image-comparison">
            <fields>
                <field type="Image Comparison: First Label" editor_type="LINE">premium_img_compare_original_img_label</field>
                <field type="Image Comparison: Second Label" editor_type="LINE">premium_image_comparison_modified_image_label</field>
            </fields>
        </widget>

        <widget name="premium-addon-image-hotspots">
            <fields-in-item items_of="premium_image_hotspots_icons">
                <field type="Hotspots: Content" editor_type="AREA">premium_image_hotspots_tooltips_texts</field>
                <field type="Hotspots: Link Title" editor_type="LINE">premium_image_hotspots_link_text</field>
                <field type="Hotspots: Content Template ID" editor_type="LINE">premium_image_hotspots_tooltips_temp</field>
                <field type="Hotspots: Hotspot Text" editor_type="LINE">premium_image_hotspots_text</field>
                <field type="Hotspots: Hotspot Link" editor_type="LINK">premium_image_hotspots_url>url</field>
            </fields-in-item>
        </widget>

        <widget name="premium-img-layers-addon">
            <fields-in-item items_of="premium_img_layers_images_repeater">
                <field type="Image Layers: Text" editor_type="LINE">img_layer_text</field>
                <field type="Image Layers: Link" editor_type="LINK">premium_img_layers_link>url</field>
            </fields-in-item>
        </widget>

        <widget name="premium-addon-magic-section">
            <fields>
                <field type="Magic Section: Content" editor_type="LINE">premium_magic_section_content</field>
                <field type="Magic Section: Content Template ID" editor_type="LINE">premium_magic_section_content_temp</field>
                <field type="Magic Section: Button Text" editor_type="AREA">premium_magic_section_button_text</field>
            </fields>
        </widget>

        <widget name="premium-multi-scroll">
            <fields>
                <field type="Multi Scroll: Dots Tooltips Text" editor_type="LINE">dots_tooltips</field>
            </fields>
            <fields-in-item items_of="left_side_repeater">
                <field type="Multi Scroll: Left Template ID" editor_type="LINE">left_side_template</field>
                <field type="Multi Scroll: Right Template ID" editor_type="LINE">right_side_template</field>
                <field type="Multi Scroll: Left Side Text" editor_type="VISUAL">left_side_text</field>
                <field type="Multi Scroll: Right Side Text" editor_type="VISUAL">right_side_text</field>
            </fields-in-item>
            <fields-in-item items_of="nav_menu_repeater">
                <field type="Navigation Menu: List Item" editor_type="LINE">nav_menu_item</field>
            </fields-in-item>
        </widget>

        <widget name="premium-hscroll">
            <fields>
                <field type="Horizontal Scroll: Dots Tooltips Text" editor_type="LINE">dots_tooltips</field>
            </fields>
            <fields-in-item items_of="section_repeater">
                <field type="Horizontal Scroll: Section ID" editor_type="LINE">section_id</field>
                <field type="Horizontal Scroll: Anchor ID" editor_type="LINE">anchor_id</field>
                <field type="Horizontal Scroll: Section Template" editor_type="LINE">section_template</field>
            </fields-in-item>
        </widget>

        <widget name="premium-notbar">
            <fields>
                <field type="Alert Box: Content Template ID" editor_type="LINE">premium_notbar_content_temp</field>
                <field type="Alert Box: Content Text" editor_type="AREA">premium_notbar_text</field>
                <field type="Alert Box: URL" editor_type="LINK">premium_notbar_link>url</field>
            </fields>
        </widget>

        <widget name="premium-addon-preview-image">
            <fields>
                <field type="Preview Window: Trigger Text" editor_type="LINE">trigger_text</field>
                <field type="Preview Window: Contnet Template ID" editor_type="LINE">premium_preview_image_content_temp</field>
                <field type="Preview Window: Caption" editor_type="LINE">premium_preview_image_caption</field>
                <field type="Preview Window: Title" editor_type="LINE">premium_preview_image_title</field>
                <field type="Preview Window: Description" editor_type="AREA">premium_preview_image_desc</field>
                <field type="Preview Window: URL" editor_type="LINK">premium_preview_image_link>url</field>
            </fields>
        </widget>

        <widget name="premium-addon-tabs">
            <fields-in-item items_of="premium_tabs_repeater">
                <field type="Tabs: Tab Template ID" editor_type="LINE">premium_tabs_content_temp</field>
                <field type="Tabs: Tab Content" editor_type="AREA">premium_tabs_content_text</field>
                <field type="Tabs: Tab Title: Section Template" editor_type="LINE">premium_tabs_title</field>
            </fields-in-item>
        </widget>

        <widget name="premium-twitter-feed">
            <fields>
                <field type="Twitter Feed: Read More Text" editor_type="LINE">read_text</field>
            </fields>
        </widget>

        <widget name="premium-yelp-reviews">
            <fields>
                <field type="Yelp Reviews: Read More Text" editor_type="LINE">readmore</field>
            </fields>
        </widget>

        <widget name="premium-unfold-addon">
            <fields>
                <field type="Unfold: Title" editor_type="LINE">premium_unfold_title</field>
                <field type="Unfold: Content" editor_type="AREA">premium_unfold_content</field>
                <field type="Unfold: Content Template ID" editor_type="LINE">content_temp</field>
                <field type="Unfold: Unfold Text" editor_type="LINE">premium_unfold_button_fold_text</field>
                <field type="Unfold: Fold Text" editor_type="LINE">premium_unfold_button_unfold_text</field>
            </fields>
        </widget>

        <widget name="premium-whatsapp-chat">
            <fields>
                <field type="Whatsapp: Phone Number" editor_type="LINE">number</field>
                <field type="Whatsapp: Group ID" editor_type="LINE">group_id</field>
                <field type="Whatsapp: Button Text" editor_type="LINE">button_text</field>
                <field type="Whatsapp: Tooltip Text" editor_type="LINE">tooltips_msg</field>
            </fields>
        </widget>

        <widget name="premium-tables-addon">
            <fields>
                <field type="Table: Search Label" editor_type="LINE">premium_table_search_placeholder</field>
                <field type="Table: Records Label" editor_type="LINE">premium_table_records_label</field>
            </fields>
            <fields-in-item items_of="premium_table_body_repeater">
                <field type="Table: Cell Text" editor_type="LINE">premium_table_text</field>
                <field type="Table: Cell Link" editor_type="LINK">premium_table_link>url</field>
            </fields-in-item>
            <fields-in-item items_of="premium_table_head_repeater">
                <field type="Table: Head Text" editor_type="LINE">premium_table_text</field>
                <field type="Table: Head Link" editor_type="LINK">head_link>url</field>
            </fields-in-item>

        </widget>

        <widget name="premium-smart-post-listing">
            <fields>
                <field type="Smart Post Listing: Empty Query Message" editor_type="LINE">empty_query_text</field>
                <field type="Smart Post Listing: Post Title" editor_type="LINE">posts_title</field>
                <field type="Smart Post Listing: Filter Tabs Message" editor_type="LINE">filter_tabs_notice</field>
                <field type="Smart Post Listing: First Tab Label" editor_type="LINE">first_tab_label</field>
                <field type="Smart Post Listing: Previous Page String" editor_type="LINE">premium_blog_prev_text</field>
                <field type="Smart Post Listing: Next Page String" editor_type="LINE">premium_blog_next_text</field>
            </fields>
        </widget>

    </elementor-widgets>
</wpml-config>
